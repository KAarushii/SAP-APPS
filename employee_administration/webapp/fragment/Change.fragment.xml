<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form"
	xmlns:core="sap.ui.core">
	<VBox id="_IDGenVBox1" class="sapUiSmallMargin">
		<f:Form id="FormChange354"
			editable="true">
			<f:title>
				<core:Title id="_IDGenTitle1" text="Address" />
			</f:title>
			<f:layout>
				<f:ResponsiveGridLayout id="_IDGenResponsiveGridLayout1"
					labelSpanXL="3"
					labelSpanL="3"
					labelSpanM="3"
					labelSpanS="12"
					adjustLabelSpan="false"
					emptySpanXL="4"
					emptySpanL="4"
					emptySpanM="4"
					emptySpanS="0"
					columnsXL="1"
					columnsL="1"
					columnsM="1"
					singleContainerFullSize="false" />
			</f:layout>
			<f:formContainers>
				<f:FormContainer id="_IDGenFormContainer1">
					<f:formElements>
						<f:FormElement id="_IDGenFormElement1" label="Name">
							<f:fields>
								<Input value="{name}" id="name"/>
							</f:fields>
						</f:FormElement>
						<f:FormElement id="_IDGenFormElement2" label="Street">
							<f:fields>
								<Input id="_IDGenInput1" value="{Street}" />
								<Input id="_IDGenInput2" value="{HouseNumber}">
									<layoutData>
										<l:GridData id="_IDGenGridData1" span="XL1 L2 M2 S4" />
									</layoutData>
								</Input>
							</f:fields>
						</f:FormElement>
						<f:FormElement id="_IDGenFormElement3" label="ZIP Code/City">
							<f:fields>
								<Input id="_IDGenInput3" value="{ZIPCode}">
									<layoutData>
										<l:GridData id="_IDGenGridData2" span="XL1 L2 M2 S4" />
									</layoutData>
								</Input>
								<Input id="_IDGenInput4" value="{City}" />
							</f:fields>
						</f:FormElement>
						<f:FormElement id="_IDGenFormElement4" label="Country">
							<f:fields>
								<Select id="country" selectedKey="{Country}">
									<items>
										<core:Item id="_IDGenItem1" text="England" key="England"/>
										<core:Item id="_IDGenItem2" text="Germany" key="Germany"/>
										<core:Item id="_IDGenItem3" text="USA" key="USA"/>
									</items>
								</Select>
							</f:fields>
						</f:FormElement>
					</f:formElements>
				</f:FormContainer>
			</f:formContainers>
		</f:Form>
	</VBox>
</core:FragmentDefinition>