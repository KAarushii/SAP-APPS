                <End Role="Department" Type="EmployeeServices.Department" Multiplicity="0..1"/>
                <ReferentialConstraint>
                    <Principal Role="Department">
                        <PropertyRef Name="ID"/>
                    </Principal>
                    <Dependent Role="Employees">
                        <PropertyRef Name="department_ID"/>
                    </Dependent>
                </ReferentialConstraint>
            </Association>
