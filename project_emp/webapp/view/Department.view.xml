<mvc:View xmlns:table="sap.ui.table" controllerName="project_emp.controller.Department"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m">
    
    <Page id="page" title="{i18n>detailPageTitle}"
    showNavButton="true" navButtonPress=".onNavBack">
    <content>
    <Table id="_IDGenTable1" items="{Department>/}" mode="SingleSelectLeft">
             <columns>
                     <Column id="_IDGenColumn14">
                        <Label id="_IDGenLabel14" text="OrderID"></Label>
                     </Column>
                     <Column id="_IDGenColumn1">
                        <Label id="_IDGenLabel1" text="CustomerID"></Label>
                     </Column>
                     <Column id="_IDGenColumn2">
                       <Label id="_IDGenLabel2" text="EmployeeID"></Label>
                      </Column>
                     <Column id="_IDGenColumn3">
                        <Label id="_IDGenLabel3" text="OrderDate"></Label>
                     </Column>
                     <Column id="_IDGenColumn4">
                        <Label id="_IDGenLabel4" text="RequiredDate"></Label>
                     </Column>
                     <Column id="_IDGenColumn5">
                        <Label id="_IDGenLabel5" text="ShippedDate"></Label>
                     </Column>
                     <Column id="_IDGenColumn6">
                        <Label id="_IDGenLabel6" text="ShipVia"></Label>
                     </Column>
                     <Column id="_IDGenColumn7">
                        <Label id="_IDGenLabel7" text="Freight"></Label>
                     </Column>
                     <Column id="_IDGenColumn8">
                        <Label id="_IDGenLabel8" text="ShipName"></Label>
                     </Column>
                     <Column id="_IDGenColumn9">
                        <Label id="_IDGenLabel9" text="ShipAddress"></Label>
                     </Column>
                     <Column id="_IDGenColumn10">
                        <Label id="_IDGenLabel10" text="ShipCity"></Label>
                     </Column>
                     <Column id="_IDGenColumn11">
                        <Label id="_IDGenLabel11" text="ShipRegion"></Label>
                     </Column>
                     <Column id="_IDGenColumn12">
                        <Label id="_IDGenLabel12" text="ShipPostalCode"></Label>
                     </Column>
                     <Column id="_IDGenColumn13">
                        <Label id="_IDGenLabel13" text="ShipCountry"></Label>
                     </Column>

             </columns>

             <items>
                  <ColumnListItem id="_IDGenColumnListItem1" type="Navigation" press="onNavToDetails" >
                        <cells>
                              <Text id="_IDGenText14" text = "{Department>OrderID}"></Text>
                              <Text id="_IDGenText1" text = "{Department>CustomerID}"></Text>
                              <Text id="_IDGenText2" text = "{Department>EmployeeID}"></Text>
                              <Text id="_IDGenText3" text = "{Department>OrderDate}"></Text>
                              <Text id="_IDGenText4" text = "{Department>RequiredDate}"></Text>
                              <Text id="_IDGenText5" text = "{Department>ShippedDate}"></Text>
                              <Text id="_IDGenText6" text = "{Department>ShipVia}"></Text>
                              <Text id="_IDGenText7" text = "{Department>Freight}"></Text>
                              <Text id="_IDGenText8" text = "{Department>ShipName}"></Text>
                              <Text id="_IDGenText9" text = "{Department>ShipAddress}"></Text>
                              <Text id="_IDGenText10" text = "{Department>ShipCity}"></Text>
                              <Text id="_IDGenText11" text = "{Department>ShipRegion}"></Text>
                              <Text id="_IDGenText12" text = "{Department>ShipPostalCode}"></Text>
                              <Text id="_IDGenText13" text = "{Department>ShipCountry}"></Text>
                        </cells>
                  </ColumnListItem>
             </items>
           </Table>
    </content>
    </Page>
</mvc:View>

